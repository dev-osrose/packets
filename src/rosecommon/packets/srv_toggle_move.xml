<packet ePacketType = "PAKWC_TOGGLE_MOVE">
    <element name = "object_id" type = "uint16_t" default = "0" />
    <element name = "type" type = "ToggleMove" />
    <element name = "run_speed" type = "uint16_t" default = "0" />

    <simpleType name = "ToggleMove">
        <restriction base = "uint8_t">
            <enumeration value = "RUN" id = "0" />
            <enumeration value = "SIT" id = "1" />
            <enumeration value = "DRIVE" id = "2" />
        </restriction>
    </simpleType>
</packet>
