<complexType name = "Header">
    <sequence>
        <element name = "type" type = "uint8_t" />
        <element name = "id" type = "uint16_t" />
        <element name = "isCreated" type = "uint8_t" />
    </sequence>
</complexType>

<complexType name = "Data">
    <sequence>
        <element name = "gem_opt" type = "uint16_t" />
        <element name = "life" type = "uint16_t" />
        <element name = "durability" type = "uint8_t" />
        <element name = "hasSocket" type = "uint8_t" />
        <element name = "isAppraised" type = "uint8_t" />
        <element name = "refine" type = "uint8_t" />
        <element name = "count" type = "uint32_t" default = "0" />
    </sequence>
</complexType>

<complexType name = "Item">
    <sequence>
        <element name = "header" type = "Header" default = "{}" />
        <element name = "data" type = "Data" default = "{}" />
    </sequence>
</complexType>
