<packet ePacketType = "PAKSS_ACCEPT_REPLY">
    <element name = "result" type = "AcceptReplyResult" />
    <element name = "randValue" type = "uint32_t" />

    <simpleType name = "AcceptReplyResult">
        <restriction base = "uint8_t">
            <enumeration value = "CONNECT" id = "1" />
            <enumeration value = "ACCEPTED" id = "2" />
            <enumeration value = "DISCONNECT" id = "3" />
            <enumeration value = "SERVERDEAD" id = "4" />
        </restriction>
    </simpleType>
</packet>
