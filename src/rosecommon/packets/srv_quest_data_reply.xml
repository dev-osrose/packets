<packet ePacketType = "PAKWC_QUEST_DATA_REPLY">
    <element name = "result" type = "QuestDataResult" />
    <element name = "slot" type = "uint8_t" />
    <element name = "id" type = "int32_t" />

    <simpleType name = "QuestDataResult">
        <restriction base = "uint8_t">
            <enumeration value = "ADD_SUCCESS" />
            <enumeration value = "ADD_FAIL" />
            <enumeration value = "REMOVE_SUCCESS" />
            <enumeration value = "REMOVE_FAIL" />
            <enumeration value = "TRIGGER_SUCCESS" />
            <enumeration value = "TRIGGER_FAIL" />
        </restriction>
    </simpleType>
</packet>