<packet ePacketType = "PAKWC_SKILL_EFFECT">
    <element name = "target" type = "uint16_t" />
    <element name = "source" type = "uint16_t" />
    <element name = "id" type = "uint16_t" />
    <element name = "amount" type = "float" />
    <!-- STB state id for this skill, only first two bits are used (0x00, 0x01, 0x02, 0x03)-->
    <element name = "state" type = "uint8_t" />
</packet>

