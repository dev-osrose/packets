<packet ePacketType = "PAKWC_BANK_MOVE_ITEM">
    <includeXml path = "item.xml" />

    <!-- Index value of < 0 ignores that item -->
    <element name = "inventory_index" type = "int16_t" />
    <element name = "bank_index" type = "int16_t" />

    <element name = "inventory_item" type = "Item" />
    <element name = "bank_item" type = "Item" />

    <!-- Inventory zuly -->
    <element name = "money" type = "int64_t" />
</packet>
