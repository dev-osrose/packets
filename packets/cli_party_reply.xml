            <enumeration value = "ACCEPT_JOIN" />
            <enumeration value = "REJECT_JOIN" />
            <enumeration value = "DESTROY" />
            <enumeration value = "FULL" />
            <enumeration value = "INVALID_LEVEL" />
            <enumeration value = "CHANGE_OWNER" />
            <enumeration value = "CHANGE_OWNER_AND_LEAVE" />
            <enumeration value = "NO_PREMIUM" />

            <enumeration value = "BAD" id = "0x80" />
            <enumeration value = "LEAVE" />
            <enumeration value = "REJOIN" />
        </restriction>
    </simpleType>
</packet>