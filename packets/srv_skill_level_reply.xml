            <enumeration value = "SUCCEEDED" />
            <enumeration value = "FAILED" />
            <enumeration value = "NO_POINTS" />
            <enumeration value = "NEED_STAT" />
            <enumeration value = "NEED_JOB" />
            <enumeration value = "NEED_SKILL" />
            <enumeration value = "NO_MONEY" />
        </restriction>
    </simpleType>
</packet>